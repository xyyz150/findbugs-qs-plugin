<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="https://findbugs.googlecode.com/svn-history/r13379/trunk/findbugs/etc/findbugsplugin.xsd"
                pluginid="com.qs.findbugs"
                defaultenabled="true"
                provider="test">
    <Detector class="com.qs.findbugs.ForbidSystemOutClass" speed="fast" reports="SYSTEM_OUT_ERROR,PRINTSTACKTRACE_ERROR" />
    <BugPattern type="SYSTEM_OUT_ERROR" abbrev="SOE" category="CORRECTNESS" />
    <BugPattern type="PRINTSTACKTRACE_ERROR" abbrev="PSTE" category="CORRECTNESS" />
</FindbugsPlugin>